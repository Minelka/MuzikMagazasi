@model YapimciViewModel
@{
    Layout = "_LayoutBoxContent";

    ViewData["LayoutHeaderTitle"] = "DETAY";
    ViewData["LayoutHeaderButtonController"] = "Yapımcı";
    ViewData["LayoutHeaderButtonAction"] = "Index";
    ViewData["LayoutHeaderButtonTitle"] = "Geri Dön";
}

@section LayoutBoxBody
{
    <dl class="row">
        <dt class="col-2">@Html.DisplayNameFor(m => m.Id)</dt>
        <dd class="col-10">@Html.DisplayTextFor(m => m.Id)</dd>

        <dt class="col-2">@Html.DisplayNameFor(m => m.Ad)</dt>
        <dd class="col-10">@Html.DisplayTextFor(m => m.Ad)</dd>

        <dt class="col-2">@Html.DisplayNameFor(m => m.Soyad)</dt>
        <dd class="col-10">@Html.DisplayTextFor(m => m.Soyad)</dd>

        <dt class="col-2">@Html.DisplayNameFor(m => m.DogumTarihi)</dt>
        <dd class="col-10">@Html.DisplayTextFor(m => m.DogumTarihi)</dd>

        <dt class="col-2">@Html.DisplayNameFor(m => m.SahismiSirketmi)</dt>
        <dd class="col-10">@Html.DisplayTextFor(m => m.SahismiSirketmi)</dd>

        <dt class="col-2">@Html.DisplayNameFor(m => m.SirketAdi)</dt>
        <dd class="col-10">@Html.DisplayTextFor(m => m.SirketAdi)</dd>

        <dt class="col-2">@Html.DisplayNameFor(m => m.Status)</dt>
        <dd class="col-10">@Html.DisplayTextFor(m => m.Status)</dd>

        <dt class="col-2">@Html.DisplayNameFor(m => m.Created)</dt>
        <dd class="col-10">@Html.DisplayTextFor(m => m.Created)</dd>

        <dt class="col-2">@Html.DisplayNameFor(m => m.Updated)</dt>
        <dd class="col-10">@Html.DisplayTextFor(m => m.Updated)</dd>
    </dl>
}
